<h1>All Tasks</h1>
     <%= link_to "New Task", new_task_path(@task), class: 'btn btn-success' %>
<% @tasks.each do |task| %>
     <div class="media">
     <div class="media-body">
         <%= task.title %>
         <%= link_to "Delete", task, method: :delete, remote: true %>
     </div>
   </div>
 <% end %>

 <script type="text/javascript">
      $(document).on("ready page:load", function () {
         blocmetrics.report("Item Completed");
       })
 </script>
